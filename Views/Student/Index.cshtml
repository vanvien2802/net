@model List<Student>
@{
    ViewData["Title"] = "Student Page";
    var students = ViewBag.StudentList as List<Student>;
        @* var students = Model; *@
}

<div class="text-center">
    <h1 class="display-4">Welcome Student Page</h1>
    <table>
        <thead>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Address</th>
            <th>Options</th>
        </thead>
        <tbody>
            @foreach (var student in students)
            {
                <tr>
                    <td>@student.Id</td>
                    <td>@student.Name</td>
                    <td>@student.Age</td>
                    <td>@student.Address</td>
                    <td>Options</td>
                </tr>
            }
        </tbody>
    </table>
</div>
